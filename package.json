{
  "name": "reactables",
  "version": "0.3.23",
  "description": "Customizable React.js components",
  "main": "lib/index.js",
  "scripts": {
    "prebuild": "rimraf lib",
    "dev": "cross-env NODE_ENV=development nodemon ./demo/server.js --exec babel-node --presets es2015,stage-0 --ignore /demo/client --ignore ./src",
    "build": "cross-env babel --out-dir lib --ignore *.test.js src && copyfiles ./src/styles/* -f lib/styles && copyfiles ./src/vendor/fonts/* -f lib/vendor/fonts",
    "test": "mocha test/*.spec.js --require test/.setup.js --compilers js:babel-core/register"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/phil3903/reactables.git"
  },
  "keywords": [
    "react",
    "react.js"
  ],
  "author": "Phil Scott",
  "license": "ISC",
  "babel": {
    "presets": [
      "es2015",
      "stage-0",
      "react"
    ]
  },
  "bugs": {
    "url": "https://github.com/phil3903/reactables/issues"
  },
  "homepage": "https://github.com/phil3903/reactables#readme",
  "dependencies": {
    "color": "^0.11.4",
    "d3": "^4.4.0",
    "d3-sankey": "^0.4.1",
    "lodash": "^4.16.6",
    "moment": "^2.15.2",
    "react": "^15.3.2",
    "react-dom": "^15.3.2",
    "react-redux": "^4.4.6",
    "redux": "^3.6.0"
  },
  "devDependencies": {
    "babel-cli": "^6.18.0",
    "babel-core": "^6.18.0",
    "babel-loader": "^6.2.5",
    "babel-polyfill": "^6.13.0",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-react": "^6.16.0",
    "babel-preset-stage-0": "^6.16.0",
    "chai": "^3.5.0",
    "chai-enzyme": "^0.5.1",
    "copyfiles": "^1.0.0",
    "cross-env": "^3.1.3",
    "css-loader": "^0.25.0",
    "enzyme": "^2.4.1",
    "expect": "^1.20.2",
    "express": "^4.14.0",
    "file-loader": "^0.9.0",
    "isomorphic-fetch": "^2.2.1",
    "jsdom": "^9.4.2",
    "mocha": "^3.0.2",
    "morgan": "^1.7.0",
    "path": "^0.12.7",
    "react-addons-test-utils": "^15.3.2",
    "rimraf": "^2.5.4",
