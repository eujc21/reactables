{
  "name": "reactables",
  "version": "0.4.1-beta.6",
  "description": "Customizable React.js components",
  "main": "lib/index.js",
  "scripts": {
    "dev": "cross-env NODE_ENV=development nodemon ./docs/server.js --exec babel-node --presets es2015,stage-0 --ignore /docs/src/client --ignore ./src --ignore ./test",
    "build-docs": "cross-env NODE_ENV=production webpack",
    "prebuild": "rimraf lib",
    "build": "cross-env babel --out-dir lib --ignore *.test.js src && copyfiles ./src/styles/* -f lib/styles && copyfiles ./src/vendor/fonts/* -f lib/vendor/fonts",
    "test": "mocha test/*.spec.js --require test/.setup.js --compilers js:babel-core/register",
    "start": "cross-env NODE_ENV=production nodemon ./docs/server.js --exec babel-node --presets es2015,stage-0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/phil3903/reactables.git"
  },
  "keywords": [
    "react",
    "react.js"
  ],
  "author": "Phil Scott",
  "license": "ISC",
  "babel": {
    "presets": [
      "es2015",
      "stage-0",
      "react"
    ]
  },
  "files": [
    "lib"
  ],
  "bugs": {
    "url": "https://github.com/phil3903/reactables/issues"
  },
  "homepage": "https://github.com/phil3903/reactables#readme",
  "dependencies": {
    "d3": "^4.7.3",
    "d3-sankey": "^0.4.2",
    "gsap": "^1.19.1",
    "lodash": "^4.17.4",
    "material-design-icons": "^3.0.1",
    "moment": "^2.17.1",
    "react": "^15.4.2",
    "react-addons-transition-group": "^15.4.2",
    "react-dom": "^15.4.2",
    "react-redux": "^5.0.3",
    "react-router-dom": "^4.0.0",
    "redux": "^3.6.0"
  },
  "devDependencies": {
    "babel-cli": "^6.24.0",
    "babel-core": "^6.24.0",
    "babel-loader": "^6.4.0",
    "babel-polyfill": "^6.23.0",
    "babel-preset-es2015": "^6.24.0",
    "babel-preset-react": "^6.23.0",
    "babel-preset-stage-0": "^6.22.0",
    "bundle-loader": "^0.5.5",
    "chai": "^3.5.0",
    "chai-enzyme": "^0.6.1",
    "copyfiles": "^1.2.0",
    "cross-env": "^3.2.3",
    "css-loader": "^0.27.3",
    "enzyme": "^2.7.1",
    "expect": "^1.20.2",
    "express": "^4.15.2",
    "file-loader": "^0.10.1",
    "html-webpack-plugin": "^2.28.0",
    "isomorphic-fetch": "^2.2.1",
    "jsdom": "^9.12.0",
    "mocha": "^3.2.0",
    "morgan": "^1.8.1",
    "path": "^0.12.7",
    "react-addons-test-utils": "^15.4.2",
    "react-router": "^4.0.0",
    "react-router-dom": "^4.0.0",
    "rimraf": "^2.6.1",
    "static-loader": "^0.1.8",
    "style-loader": "^0.13.2",
    "url-loader": "^0.5.8",
    "webpack": "^2.2.1",
    "webpack-dev-middleware": "^1.10.1",
    "webpack-hot-middleware": "^2.17.1"
  }
}
