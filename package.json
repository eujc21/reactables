{
  "name": "reactables",
  "version": "0.4.1-beta.9",
  "description": "Customizable React.js components",
  "main": "lib/index.js",
  "scripts": {
    "dev": "cross-env NODE_ENV=development nodemon ./src/docs/dev-server.js --exec babel-node --presets es2015,stage-0  --ignore ./src --ignore ./test",
    "prebuild-docs": "rimraf docs",
    "build-docs": "cross-env NODE_ENV=production webpack",
    "prebuild": "rimraf lib",
    "build": "cross-env NODE_ENV=production babel --out-dir lib --ignore *.test.js --ignore docs --ignore src/docs src/lib && copyfiles ./src/lib/styles/* -f ./lib/styles && copyfiles ./src/lib/vendor/fonts/* -f ./lib/vendor/fonts",
    "test": "cross-env mocha test/*.spec.js --require test/.setup.js --compilers js:babel-core/register",
    "start": "cross-env NODE_ENV=production nodemon .src/docs/dev-server.js --exec babel-node --presets es2015,stage-0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/phil3903/reactables.git"
  },
  "keywords": [
    "react",
    "react.js"
  ],
  "author": "Phil Scott",
  "license": "ISC",
  "babel": {
    "presets": [
      "stage-0",
      "es2015"
    ]
  },
  "files": [
    "lib"
  ],
  "bugs": {
    "url": "https://github.com/phil3903/reactables/issues"
  },
  "homepage": "https://github.com/phil3903/reactables#readme",
  "dependencies": {
    "d3": "^4.7.3",
    "d3-sankey": "^0.4.2",
    "faker": "^4.1.0",
    "gsap": "^1.19.1",
    "immutable": "^3.8.1",
    "lodash": "^4.17.4",
    "material-design-icons": "^3.0.1",
    "moment": "^2.17.1",
    "react-addons-transition-group": "^15.4.2",
    "react-redux": "^5.0.3",
    "redux": "^3.6.0"
  },
  "peerDependencies": {
    "react": "^0.14.0 || ^15.0.0",
    "react-dom": "^0.14.0 || ^15.0.0"
  },
  "devDependencies": {
    "babel-cli": "^6.24.0",
    "babel-core": "^6.24.0",
    "babel-loader": "^6.4.1",
    "babel-plugin-transform-async-to-generator": "^6.22.0",
    "babel-plugin-transform-regenerator": "^6.22.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-polyfill": "^6.23.0",
    "babel-preset-es2015": "^6.24.0",
    "babel-preset-react": "^6.23.0",
    "babel-preset-stage-0": "^6.22.0",
    "bundle-loader": "^0.5.5",
    "chai": "^3.5.0",
    "chai-enzyme": "^0.6.1",
    "copyfiles": "^1.2.0",
    "cross-env": "^3.2.3",
    "css-loader": "^0.27.3",
    "dotenv-cli": "^1.1.1",
    "enzyme": "^2.7.1",
    "expect": "^1.20.2",
    "express": "^4.15.2",
    "file-loader": "^0.11.1",
    "html-webpack-plugin": "^2.28.0",
    "jsdom": "^9.12.0",
    "kcors": "^1.3.2",
    "koa": "^2.2.0",
    "koa-bodyparser": "^4.2.0",
    "koa-webpack": "^0.3.1",
    "mocha": "^3.2.0",
    "morgan": "^1.8.1",
    "path": "^0.12.7",
    "react": "^15.4.2",
    "react-addons-test-utils": "^15.4.2",
    "react-dom": "^15.4.2",
